{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/qr",
      "dest": "index.js"
    },
    {
      "src": "/pair",
      "dest": "index.js"
    },
    {
      "src": "/valid",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}
